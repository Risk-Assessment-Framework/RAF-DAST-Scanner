{
  "name": "CGI-bin directory found",
  "run_at": "fs",
  "find": "once",
  "severity": 0,
  "options": ["discovery"],
  "data": {
    "url": "cgi-bin/",
    "options": [
      "rootdir"
    ]
  },
  "request": null,
  "matches": [
    {
      "type": "regex",
      "location": "body",
      "match": ".+?",
      "name": "CGI-bin Directory found",
      "options": [
        "ignore_case"
      ]
    }
  ]
}